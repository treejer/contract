    "big.js": "6.1.1",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.1.1",
    "dotenv": "16.0.0",
    "eth-gas-reporter": "0.2.24",
    "ethereumjs-units": "0.2.0",
